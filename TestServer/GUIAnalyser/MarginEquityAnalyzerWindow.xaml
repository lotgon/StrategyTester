<Window x:Class="GUIAnalyser.MarginEquityAnalyzerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:sChooser="clr-namespace:GUIAnalyser"
        Title="MarginEquityAnalyzerWindow" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <sChooser:SymbolChooser x:Name="symbolChooser" Width="Auto"/>
        <StackPanel Grid.Column="1">
            <Button Name="ButtonShow3DStudents" Click="ButtonShow3DStudents_Click">Show 3D Students</Button>
        </StackPanel>

    </Grid>
</Window>
